    "license": "MIT",
    "scripts": {
        "lint": "eslint --ext ts,tsx src test",
        "copy:paste:detector": "jscpd src test typings",
        "compile": "tsc",
        "develop": "GATSBY_GRAPHQL_IDE=playground concurrently --kill-others \"gatsby develop\" \"npm:start:static-server\"",
        "build": "gatsby build",
        "build:visual": "GATSBY_VISUAL_REGRESSION_TEST=true gatsby build",
        "serve": "gatsby serve",
        "test:e2e": "testcafe test/e2e/",
        "test:e2e:ci": "testcafe 'browserstack:chrome:Windows 10','browserstack:firefox:Windows 10' test/e2e/",
        "test:visual": "percy exec -- testcafe chrome:headless test/visual/",
        "test:unit": "ava",
        "test:unit:coverage": "c8 ava",
        "test:a11y": "pa11y",
        "release": "standard-version",
        "start:static-server": "ts-node-dev --transpile-only src/static-server.ts"
    },
    "dependencies": {
        "@fontsource/jetbrains-mono": "4.5.0",
