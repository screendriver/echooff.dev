    "scripts": {
        "lint": "eslint --ext ts,tsx src test",
        "copy:paste:detector": "jscpd src test typings",
        "compile": "tsc",
        "check:astro": "astro check",
        "check:svelte": "svelte-check",
        "astro": "astro dev",
        "develop": "FORCE_COLOR='1' ts-node-esm ./src/start-develop.ts",
        "build": "astro build",
        "preview": "astro preview",
        "test:unit": "vitest test/unit/**/*",
        "test:unit:coverage": "vitest run --coverage test/unit/**/*",
        "test:e2e": "playwright test --config=test/e2e/playwright.config.ts",
        "test:visual-regression": "concurrently --success first --kill-others \"npm run develop\" \"wait-on http://localhost:8888 && percy exec --config ./test/visual-regression/percy.config.json -- ts-node-esm --transpileOnly ./test/visual-regression/main.test.ts/\""
    },
