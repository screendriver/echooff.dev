{
  "name": "echooff.dev",
  "private": true,
  "version": "0.0.1",
  "main": "index.js",
  "repository": "git@github.com:screendriver/echooff.dev.git",
  "author": "Christian Rackerseder <git@echooff.de>",
  "license": "MIT",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts?(x)' 'test/**/*.ts?(x)'",
    "prettier:check": "prettier --check 'src/**/*.ts?(x)' 'test/**/*.ts?(x)' 'typings/**/*.ts'",
    "copy:paste:detector": "jscpd src test typings",
    "compile": "tsc && tsc -p src/functions/",
    "develop": "GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "build": "gatsby build",
    "build:visual": "GATSBY_VISUAL_REGRESSION_TEST=true gatsby build",
    "serve": "gatsby serve",
    "test:e2e": "testcafe chrome:headless 'test/e2e/**/*.ts' --app 'serve public'",
    "test:visual": "node -r esm target/test/visual/run.js test",
    "test:visual:approve": "node -r esm target/test/visual/run.js approve",
    "test:unit:base": "tape -r esm -r source-map-support/register -r jsdom-global/register -r './target/test/setup.js' 'target/test/unit/**/*.js'",
    "test:unit": "yarn test:unit:base | tap-nirvana",
    "test:unit:ci": "npm run test:unit:coverage | tap-xunit > target/test-results.xml",
    "test:unit:coverage": "nyc npm run test:unit:base"
  },
  "dependencies": {
    "@emotion/core": "10.0.15",
    "@emotion/styled": "10.0.15",
    "@types/aws-lambda": "8.10.31",
    "form-urlencoded": "4.0.0",
    "gatsby": "2.13.44",
    "gatsby-image": "2.2.7",
    "got": "9.6.0",
    "gsap": "2.1.3",
    "hex-to-rgba": "2.0.1",
    "ky": "0.11.2",
    "lodash.sample": "4.2.1",
    "modern-normalize": "0.5.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-feather": "2.0.3",
    "react-helmet": "5.2.1",
    "typeface-lato": "0.0.75",
    "typeface-open-sans": "0.0.75",
    "typeface-source-sans-pro": "0.0.75"
  },
  "devDependencies": {
    "@testing-library/react": "8.0.7",
    "@types/form-urlencoded": "2.0.1",
    "@types/got": "9.6.5",
    "@types/gsap": "1.20.2",
    "@types/lodash.sample": "4.2.6",
    "@types/micro": "7.3.3",
    "@types/react": "16.8.24",
    "@types/react-dom": "16.8.5",
    "@types/react-helmet": "5.0.8",
    "@types/sinon": "7.0.13",
    "@types/tape": "4.2.33",
    "backstopjs": "4.1.9",
    "esm": "3.2.25",
    "gatsby-plugin-emotion": "4.1.2",
    "gatsby-plugin-netlify": "2.1.3",
    "gatsby-plugin-react-helmet": "3.1.2",
    "gatsby-plugin-robots-txt": "1.5.0",
    "gatsby-plugin-sharp": "2.2.9",
    "gatsby-plugin-sitemap": "2.2.4",
    "gatsby-plugin-typescript": "2.1.2",
    "gatsby-source-filesystem": "2.1.6",
    "gatsby-source-graphql": "2.1.3",
    "gatsby-transformer-json": "2.2.2",
    "gatsby-transformer-sharp": "2.2.5",
    "jscpd": "2.0.15",
    "jscpd-badge-reporter": "1.1.3",
    "jsdom": "15.1.1",
    "jsdom-global": "3.0.2",
    "micro": "9.3.4",
    "netlify-cli": "2.12.0",
    "nyc": "14.1.1",
    "prettier": "1.18.2",
    "serve": "11.1.0",
    "serve-handler": "6.1.0",
    "sinon": "7.3.2",
    "source-map-support": "0.5.13",
    "tap-nirvana": "1.1.0",
    "tap-xunit": "2.4.1",
    "tape": "4.11.0",
    "testcafe": "1.3.3",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "4.0.0",
    "typescript": "3.5.3"
  }
}
