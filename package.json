	"scripts": {
		"lint": "eslint . --ext '.ts,.tsx'",
		"copy:paste:detector": "jscpd src test",
		"compile": "tsc",
		"check:astro": "astro check",
		"check:svelte": "svelte-check",
		"astro": "astro dev --port $PORT",
		"develop": "FORCE_COLOR='1' tsx ./src/start-develop.ts",
		"build": "astro build",
		"preview": "astro preview",
		"test:unit": "vitest --dir test/unit/",
		"test:unit:coverage": "vitest run --coverage --dir test/unit/",
		"test:e2e": "playwright test --config=test/e2e/playwright.config.ts"
	},
