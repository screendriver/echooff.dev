        "@testing-library/react": "12.1.2",
        "@types/gsap": "1.20.2",
        "@types/node": "14.17.27",
        "@types/react": "17.0.31",
        "@types/react-dom": "17.0.10",
        "@types/react-helmet": "6.1.4",
        "@types/sinon": "10.0.4",
        "@types/standard-version": "7.0.1",
        "@typescript-eslint/eslint-plugin": "4.31.2",
        "@typescript-eslint/parser": "4.31.2",
        "autoprefixer": "10.4.0",
        "ava": "3.15.0",
