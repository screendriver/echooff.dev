        "prettier": "2.5.1",
        "react-test-renderer": "17.0.2",
        "sinon": "13.0.1",
        "standard-version": "9.3.2",
        "tailwind-dracula": "1.0.7",
        "testcafe": "1.18.4",
        "testcafe-browser-provider-browserstack": "1.13.1",
        "ts-node": "10.5.0",
        "ts-node-dev": "1.1.8",
        "typescript": "4.5.5",
        "wait-on": "6.0.1"
    },
    "overrides": {
        "graphql-compose": "7.25.1"
    }
}
