    "scripts": {
        "lint": "eslint --ext ts,tsx src test",
        "copy:paste:detector": "jscpd src test typings",
        "compile": "tsc",
        "develop": "GATSBY_GRAPHQL_IDE=playground gatsby develop",
        "build": "gatsby build",
        "serve": "gatsby serve",
        "test:unit": "ava",
        "test:unit:coverage": "c8 ava",
        "test:e2e": "concurrently --success first --kill-others \"npm run develop\" \"wait-on http://localhost:8000 && playwright test --config=test/e2e/playwright.config.ts\"",
        "test:visual-regression": "concurrently --success first --kill-others \"npm run develop\" \"wait-on http://localhost:8000 && percy exec --config ./test/visual-regression/percy.config.json -- ts-node --transpileOnly ./test/visual-regression/main.test.ts/\""
    },
