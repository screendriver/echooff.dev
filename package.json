{
    "name": "echooff.dev",
    "private": true,
    "version": "0.0.1",
    "main": "index.js",
    "repository": "git@github.com:screendriver/echooff.dev.git",
    "author": "Christian Rackerseder <git@echooff.de>",
    "license": "MIT",
    "scripts": {
        "lint": "eslint --ext ts,tsx src test",
        "copy:paste:detector": "jscpd src test typings",
        "compile": "tsc",
        "develop": "GATSBY_GRAPHQL_IDE=playground GATSBY_GITHUB_API_BASE_URL=http://localhost:3000 GATSBY_GITHUB_LOGIN=foo GATSBY_GITHUB_API_TOKEN=test-token GATSBY_CONTACT_FORM_URL=http://localhost:3000/contact-form concurrently --kill-others \"gatsby develop\" \"npm:start:static-server\"",
        "build": "gatsby build",
        "build:visual": "GATSBY_VISUAL_REGRESSION_TEST=true gatsby build",
        "serve": "gatsby serve",
        "test:e2e": "testcafe test/e2e/",
        "test:e2e:ci:serve": "concurrently --kill-others \"npm:serve\" \"npm:start:static-server\"",
        "test:e2e:ci": "testcafe 'browserstack:chrome:Windows 11','browserstack:firefox:Windows 11','browserstack:safari:OS X Monterey' test/e2e/",
        "test:visual": "percy exec -- testcafe chrome:headless test/visual/",
        "test:unit": "ava",
        "test:unit:coverage": "c8 ava",
        "test:a11y": "concurrently --kill-others \"npm:serve\" \"npm:start:static-server\" \"wait-on http://localhost:9000 && pa11y http://localhost:9000\"",
        "release": "standard-version",
        "start:static-server": "ts-node-dev --transpile-only src/static-server.ts"
    },
    "dependencies": {
        "@fontsource/jetbrains-mono": "4.5.3",
        "@fontsource/source-sans-pro": "4.5.3",
        "@octokit/graphql": "4.8.0",
        "@xstate/react": "1.6.3",
        "date-fns": "2.28.0",
        "gatsby": "4.9.0",
        "gsap": "2.1.3",
        "ky": "0.28.7",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-helmet": "6.1.0",
        "react-icons": "4.3.1",
        "tailwindcss": "3.0.22",
        "true-myth": "5.1.2",
        "xstate": "4.28.1",
        "zod": "3.11.6"
    },
    "devDependencies": {
        "@netlify/plugin-gatsby": "2.1.2",
        "@percy/cli": "1.0.0-beta.70",
        "@percy/testcafe": "1.0.1",
        "@types/common-tags": "1.8.1",
        "@types/gsap": "1.20.2",
        "@types/node": "17.0.17",
        "@types/react": "17.0.39",
        "@types/react-dom": "17.0.11",
        "@types/react-helmet": "6.1.5",
        "@types/react-test-renderer": "17.0.1",
        "@types/sinon": "10.0.11",
        "@types/standard-version": "7.0.1",
        "@typescript-eslint/eslint-plugin": "5.11.0",
        "@typescript-eslint/parser": "5.11.0",
        "autoprefixer": "10.4.2",
        "ava": "4.0.1",
        "c8": "7.11.0",
        "common-tags": "1.8.2",
        "concurrently": "7.0.0",
        "esbuild": "0.14.21",
        "esbuild-register": "3.3.2",
        "eslint": "8.9.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-ava": "13.2.0",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-react": "7.28.0",
        "eslint-plugin-testcafe-community": "2.0.0",
        "fastify": "3.27.1",
        "fastify-cors": "6.0.2",
        "fastify-formbody": "5.2.0",
        "fishery": "2.1.0",
        "gatsby-plugin-image": "2.9.0",
        "gatsby-plugin-netlify": "4.1.0",
        "gatsby-plugin-postcss": "5.9.0",
        "gatsby-plugin-react-helmet": "5.9.0",
        "gatsby-plugin-robots-txt": "1.7.0",
        "gatsby-plugin-sharp": "4.9.0",
        "gatsby-plugin-sitemap": "5.9.0",
        "gatsby-source-filesystem": "4.9.0",
        "gatsby-source-graphql": "4.9.0",
        "gatsby-transformer-json": "4.9.0",
        "gatsby-transformer-sharp": "4.9.0",
        "jscpd": "3.4.5",
        "jscpd-badge-reporter": "1.1.3",
        "netlify-cli": "9.12.3",
        "pa11y": "6.1.1",
