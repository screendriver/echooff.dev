{
  "name": "echooff.dev",
  "private": true,
  "version": "0.0.1",
  "main": "index.js",
  "repository": "git@github.com:screendriver/echooff.dev.git",
  "author": "Christian Rackerseder <git@echooff.de>",
  "license": "MIT",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts?(x)' 'test/**/*.ts?(x)'",
    "prettier:check": "prettier --check 'src/**/*.ts?(x)' 'test/**/*.ts?(x)' 'typings/**/*.ts'",
    "copy:paste:detector": "jscpd src test typings",
    "compile": "tsc",
    "develop": "GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "build": "gatsby build",
    "build:visual": "GATSBY_VISUAL_REGRESSION_TEST=true gatsby build",
    "serve": "gatsby serve",
    "test:e2e": "testcafe chrome:headless 'test/e2e/**/*.ts' --app 'serve public'",
    "test:visual": "node dist/test/visual/run.js test",
    "test:visual:approve": "node dist/test/visual/run.js approve",
    "test:unit": "nyc tape -r source-map-support/register -r jsdom-global/register -r './dist/test/setup.js' 'dist/test/unit/**/*.js'",
    "test:unit:ci": "yarn test:unit | tap-xunit > dist/test-results.xml"
  },
  "dependencies": {
    "@emotion/core": "10.0.10",
    "@emotion/styled": "10.0.10",
    "form-urlencoded": "3.0.0",
    "gatsby": "2.3.30",
    "gatsby-image": "2.0.40",
    "gsap": "2.1.2",
    "ky": "0.10.0",
    "lodash.sample": "4.2.1",
    "modern-normalize": "0.5.0",
    "rc-progress": "2.3.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-feather": "1.1.6",
    "react-helmet": "5.2.0",
    "typeface-lato": "0.0.54",
    "typeface-open-sans": "0.0.54",
    "typeface-source-sans-pro": "0.0.54"
  },
  "devDependencies": {
    "@types/form-urlencoded": "2.0.0",
    "@types/gsap": "1.20.2",
    "@types/lodash.sample": "4.2.6",
    "@types/micro": "7.3.3",
    "@types/rc-progress": "2.2.1",
    "@types/react": "16.8.13",
    "@types/react-dom": "16.8.4",
    "@types/react-helmet": "5.0.8",
    "@types/sinon": "7.0.11",
    "@types/tape": "4.2.33",
    "backstopjs": "4.0.3",
    "gatsby-plugin-emotion": "4.0.6",
    "gatsby-plugin-netlify": "2.0.15",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-robots-txt": "1.4.0",
    "gatsby-plugin-sharp": "2.0.35",
    "gatsby-plugin-sitemap": "2.0.12",
    "gatsby-plugin-typescript": "2.0.13",
    "gatsby-source-filesystem": "2.0.32",
    "gatsby-source-graphql": "2.0.18",
    "gatsby-transformer-json": "2.1.11",
    "gatsby-transformer-sharp": "2.1.18",
    "jscpd": "2.0.14",
    "jscpd-badge-reporter": "1.1.3",
    "jsdom": "15.0.0",
    "jsdom-global": "3.0.2",
    "micro": "9.3.3",
    "netlify-cli": "2.11.12",
    "nyc": "14.0.0",
    "prettier": "1.17.0",
    "react-testing-library": "6.1.2",
    "serve": "11.0.0",
    "serve-handler": "6.0.0",
    "sinon": "7.3.2",
    "source-map-support": "0.5.12",
    "tap-xunit": "2.3.0",
    "tape": "4.10.1",
    "testcafe": "1.1.3",
    "tslint": "5.16.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "4.0.0",
    "typescript": "3.4.5"
  }
}
